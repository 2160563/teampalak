<body>

  <header>
    <!-- Navbar
    ================================================== -->
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <!-- logo -->
          <a class="brand logo" href="/">
            <img src="assets/img/logo.png" alt="" />
          </a>
          <!-- end logo -->

          <!-- top menu -->
          <div>
            <nav>
              <ul class="nav topnav">
                <li class="dropdown success">
                  <a href="/client"><i class="icon-home icon-white"></i> Home</a>
                </li>
        <li class="dropdown warning">
                  <a href="/login_tournaments"><i class="icon-bullhorn icon-white"></i> Tournaments</a>
                </li>
        
           <li class="dropdown success">
                  <a href="/results"><i class="icon-leaf icon-white"></i> Result</a>
           </li>
           <li class="dropdown warning">
              <a href="/history"><i class="icon-bullhorn icon-white"></i> History</a>
            </li>
            <li class="dropdown primary">
                
                  <a href="" id="notificationLink" onclick="setTimeout(4000);"><i class="icon-star icon-white"></i> Notifications</a>
                  <% if(num.length > 0) { %> 
                    <span id="notification_count"><%= num.length %></span>  
                  <%}%>
                  <div id="notificationContainer" style="overflow: scroll; width: 400px;
                  height: 500px;">
                    <br>
                    <br>
                    <br>
                    <div id="notificationTitle">Notifications </div>
                    <div id="notificationsBody" class="notifications">
                      <br>
                      
                      <% if(notif.length > 0) { %> 
                        <% for(var i = 0; i < notif.length; i++){%>
                          <% if(notif[i].status == "Checked"){%>
                            <div class="alert alert-info">
                               <%= notif[i].message%>
                          </div>
                          <% }else if(notif[i].status == "Unchecked"){%>
                            <div class="alert alert-warning">
                                <%= notif[i].message%>
                           </div>
                          <%}%>
                        <%}%>
                      <%}%>
                    </div>
                  </div>
                </li>
            <li class="dropdown info active">
                  <a href="#"><i class="icon-list icon-white"></i>My Account</a>
                  <ul class="dropdown-menu">
                    <li><a href="/userProfile">View Account</a></li>
                    <li><a href="/editProfile">Edit Profile</a></li>
                    <li><a href="/logout">Sign Out</a></li>
                  </ul>
                </li>
        
                  </ul>
                
               
              </ul>
            </nav>
          </div>
          <!-- end menu -->
        </div>
      </div>
    </div>
  </header>

   <section id="subintro">
    <div class="jumbotron subhead" id="overview">
      <div class="container">
        <div class="row">
          <div class="span8">
      <h1 id="tr">Profile:</h1>
            <p>“Be yourself; everyone else is already taken.” ― Oscar Wilde</p>
          </div>
          <div class="span4">
            <div class="input-append">
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  
   <section id="maincontent">
    <div class="container">
      <div class="row">
        <div class="span12">
  <div class="editpage">
    <div class="form">
      <form action="/userProfile" method="post">
        <div class="UserImage">
          <% for(var index = 0; index < data.length; index++){ %>
            <% if(data[index].Image !== null){%>
              <img src="data:image/jpeg; base64, <%=new Buffer(data[index].Image,'binary').toString('base64')%>"  alt="Profile Pic" class="imgprof" style="width: 200px; height: 200px;">
            <%}else{%>
              <img src="http://www.clker.com/cliparts/d/L/P/X/z/i/no-image-icon-hi.png"  alt="Profile Pic" class="imgprof" style="width: 200px; height: 200px;">
            <%}%>
          <h1> <%= name%> </h1>
        </div>
          <h3><b>Username: </b> <%= user%> </h3>
          
          <%}%>

          <h2 id="game">In Game Name:</h2>
                    <% if(data2.length > 1){ %>
                      <% if(data2[0].Game == "LoL"){ %>
                          <h5><b>Current In Game Name For League of Legends: </b><%= data2[0].InGameName%></h5>
                        <%}%>
                        <% if(data2[1].Game == "LoL"){ %>
                          <h5><b>Current In Game Name For League of Legends: </b><%= data2[1].InGameName%></h5>
                        <%}%>
                    <%}else if(data2.length == 1){%>
                      <% if(data2[0].Game == "LoL"){ %>
                          <h5><b>Current In Game Name For League of Legends: </b><%= data2[0].InGameName%></h5>
                        <%}%>
                    <%}%>
                    <% if(data2.length > 1){ %>
                      <% if(data2[0].Game == "Dota 2"){ %>
                          <h5><b>Current In Game Name For Dota 2: </b><%= data2[0].InGameName%></h5>
                        <%}%>
                        <% if(data2[1].Game == "Dota 2"){ %>
                          <h5><b>Current In Game Name For Dota 2: </b><%= data2[1].InGameName%></h5>
                        <%}%>
                    <%}else if(data2.length == 1){%>
                      <% if(data2[0].Game == "Dota 2"){ %>
                          <h5><b>Current In Game Name For Dota 2: </b><%= data2[0].InGameName%></h5>
                        <%}%>
                    <%}%>
              <!-- Code snippet -->
          <h3>Registered Tournaments:</h3>

            <div class="row-fluid">
              <ul class="portfolio-area da-thumbs">
              <% if(ctr1.length > 0){ %> 
                <% for(var index = 0; index < ctr1.length; index++){ %>
                <li>
                  <div class="col">
                    <div class="thumbnail">
                      <div class="tooltip-demo image-wrapp">
                        <% if(ctr1[index].Tpic!=undefined){%>
                        <img src="data:image/jpeg; base64, <%=new Buffer(ctr1[index].Tpic,'binary').toString('base64')%>" alt="">
                        <%}else{%>
                        <img src="images/logo.png" alt="" style="width:318; height:420;">
                        <%}%>
                        <div class="caption">
                          <h3>
                            <%= ctr1[index].TournamentName%>
                          </h3>
                          <%if(ctr1.length > 0){%>
                            <%if(ctr1[index].tournamentID == ctr3[index].tournamentID){%>
                              <br>Slots Left :  <%= ctr3[index].registeredteamID%>/<%= ctr3[index].Max_participants%>
                            <%}else{%>
                              <br>Slots Left :  0/<%= ctr1[index].Max_participants%>
                            <%}%>
                          <%}%>
                          <br> Tournament Range: <%=ctr1[index].TournaRange%>
                          <br> Game: <%= ctr1[index].TournamentGame%>
                          <br> Registration price:  &#8369; <%= ctr1[index].registration_fee.toLocaleString(undefined, {minimumFractionDigits: 2, maximumFractionDigits: 2 })%>
                          </p><br> 
                          <p><a data-toggle="modal" href="#myModal1<%=index%>" class="btn btn-success">More Info</a></p>
                        </div>
                      </div>
                    </div>
                  </div>
                </li>
                <% } %>
              <% } %>  
              </ul>
            </div>
          <!-- End of code snippet -->

          
          <h3>History</h3>

            
            <div class="row-fluid">
              <ul class="portfolio-area da-thumbs">
              <% if(ctr.length > 0){ %> 
                <% for(var index = 0; index < ctr.length; index++){ %>
                <li>
                  <div class="col">
                    <div class="thumbnail">
                      <div class="tooltip-demo image-wrapp">
                        <img src="data:image/jpeg; base64, <%=new Buffer(ctr[index].Tpic,'binary').toString('base64')%>" alt="">
                        <div class="caption">
                          <h3>
                          
                            <%= ctr[index].TournamentName.substring(0,11) %>
                          </h3>
                          <p>No. of participants :  <%= ctr[index].Max_participants%>
                          <br> Game: <%= ctr[index].TournamentGame%>
                          <br> Registration price: <%= ctr[index].registration_fee%>
                          </p><br> 
                          <p><a data-toggle="modal" href="#myModal<%=index%>" class="btn btn-success">More Info</a></p>
                        </div>
                      </div>
                    </div>
                  </div>
                </li>
                <% } %>
              <% } %>  
              </ul>
            </div>
    
        </div>
      </form>
    </div>
  </div>
  </div>
  </div>
  </section>
  <% for(var index = 0; index < ctr.length; index++){ %>
  <div id="myModal<%=index%>" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
              <h3 id="myModalLabel"><center><%= ctr[index].TournamentName %></center></h3>
            </div>
      <div class="modal-body">
        <h4>Game: <%= ctr[index].TournamentGame%></h4>
        <p>No. of participants :  <%= ctr[index].Max_participants%>
          <br> Slots Left: 
          <br> Registration price: <%= ctr[index].registration_fee%>
          <br> Venue: <%= ctr[index].TVenue%>
          <br> Date: <%= ctr[index].TSched.toDateString()%>
          
          <br> Time: <%= ctr[index].TSched.toLocaleTimeString('en-US', 
                  { hour12: true, 
                    hour: "numeric", 
                    minute: "numeric"});%>
        </p>
        <hr>
        <h4>Bracketing</h4>
        <hr>
        <h4>Winners:</h4>
        <hr>
      </div>
    <div class="modal-footer">
      <button class="btn" data-dismiss="modal">Close</button>
    </div>
  </div>
<% } %>

<!-- model snip -->
<% for(var index = 0; index < ctr1.length; index++){ %>
  <div id="myModal1<%=index%>" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
              <h3 id="myModalLabel"><center><%= data2[index].TournamentName %></center></h3>
            </div>
      <div class="modal-body">
        <h4>Game: <%= ctr1[index].TournamentGame%></h4>
        <%if(ctr1.length > 0){%>
          <%if(ctr1[index].tournamentID == ctr3[index].tournamentID){%>
            <br>Slots Left :  <%= ctr3[index].registeredteamID%>/<%= ctr3[index].Max_participants%>
          <%}else{%>
            <br>Slots Left :  0/<%= ctr1[index].Max_participants%>
          <%}%>
        <%}%>
          <br> Registration price: &#8369; <%= ctr1[index].registration_fee.toLocaleString(undefined, {minimumFractionDigits: 2, maximumFractionDigits: 2 })%>
          <br> Venue: <%= ctr1[index].TVenue%>
          <br> Date: <%= ctr1[index].TSched.toDateString()%>
          
          <br> Time: <%= ctr1[index].TSched.toLocaleTimeString('en-US', 
                  { hour12: true, 
                    hour: "numeric", 
                    minute: "numeric"});%>
          <br> 1st Prize:  &#8369; <%= ctr1[index].first.toLocaleString(undefined, {minimumFractionDigits: 2, maximumFractionDigits: 2 })%>
          <br> 2nd Prize:  &#8369; <%= ctr1[index].second.toLocaleString(undefined, {minimumFractionDigits: 2, maximumFractionDigits: 2 })%>
        </p>
        <hr>
        <h4>Bracketing</h4>
        <a href="bracket?tid=<%= ctr1[index].TournamentID %>" class="btn btn-success" role="button">Show bracketing </a>
      </div>
    <div class="modal-footer">
      <button class="btn" data-dismiss="modal">Close</button>
    </div>
  </div>
<% } %>
<!-- end of modal snip -->
