   <section id="maincontent">
    <div class="container">
      <div class="row">
        <div class="span12">
          <div class="editpage container">
            <div class="form">
              <form action="/uploadProfile" method="POST">
                <div class="UserImage">
                  <h6>Update profile picture</h6>
                  <input type="file" name="fileToUpload" id="fileToUpload">
                </div>
                <button class="btn" type="submit">Save Profile Picture</button>
                <% if (message3.length > 0) { %>
                        <div class="alert alert-danger text-center"><%= message3 %></div>
                <% } %>
              </form>
              <form action="/editProfile" method="POST">
                <br>
                  <h6>User Name: <b> <%= user%></b></h6> 
                  <br>
                  <h6>New Password:</h6>
                  <input type="password" placeholder="Enter new Password" name="newPwd">
                  <h6>Confirm New Password:</h6>
                  <input type="password" placeholder="Confirm new Password" name="conPwd">
                  <% if (message.length > 0) { %>
                        <div class="alert alert-danger text-center"><%= message %></div>
                  <% } %>
                  <br><br>
                  <button class="btn" type="submit">Save Password</button>
              </form>
              <form action="/changeInGameNameLol" method="POST">
                  <div class="lol">
                    <h1 id="game">In Game Name</h1>
                    <% if(data.length > 1){ %>
                      <% if(data[0].Game == "LoL"){ %>
                          <h5>Current In Game Name: <%= data[0].InGameName%></h5>
                        <%}%>
                        <% if(data[1].Game == "LoL"){ %>
                          <h5>Current In Game Name: <%= data[1].InGameName%></h5>
                        <%}%>
                    <%}else if(data.length == 1){%>
                      <% if(data[0].Game == "LoL"){ %>
                          <h5>Current In Game Name: <%= data[0].InGameName%></h5>
                        <%}%>
                    <%}%>
                    <label>League of Legends:</label>
                    <input type="text" placeholder="League of Legends" name="lol" autocomplete="off">
                    <br>
                    <% if (message1.length > 0) { %>
                        <div class="alert alert-danger text-center"><%= message1 %></div>
                    <% } %>
                    <button class="btn" type="submit">Save In Game Name</button>
                    <br>
                    <br>
                  </div>
              </form>
              <form action="/changeInGameNameDota2" method="POST">
                  <div class="dota2">
                  <% if(data.length > 1){ %>
                    <% if(data[0].Game == "Dota 2"){ %>
                        <h5>Current In Game Name: <%= data[0].InGameName%></h5>
                      <%}%>
                      <% if(data[1].Game == "Dota 2"){ %>
                        <h5>Current In Game Name: <%= data[1].InGameName%></h5>
                      <%}%>
                  <%}else if(data.length == 1){%>
                    <% if(data[0].Game == "Dota 2"){ %>
                        <h5>Current In Game Name: <%= data[0].InGameName%></h5>
                      <%}%>
                  <%}%>
                    <label>Dota 2:</label>
                    <input type="text" placeholder="DotA 2" name="dota2" autocomplete="off">
                  <br>
                  <% if (message2.length > 0) { %>
                        <div class="alert alert-danger text-center"><%= message2 %></div>
                  <% } %>
                  <button class="btn" type="submit">Save In Game Name</button>
                  </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>


